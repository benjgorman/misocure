<%= form_for @album do |f| %>
	<%= render 'shared/error_messages', :object => f.object %>
	<div class="field">
		<%= f.label :name %><br />
		<%= f.text_field :name %>
	</div>
	<div class="field">
		<%= f.label :desc %><br />
		<%= f.text_area :desc %>
	</div>
	<div class = "actions">
		<%= f.submit "Submit"%>
	</div>
	<% end %>
