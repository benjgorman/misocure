<%= form_for @artist do |f| %>
	<%= render 'shared/error_messages', :object => f.object %>
	<div class="field">
		<%= f.label :name %><br />
		<%= f.text_area :name %>
		
	</div>
	
	<div class="field">
		<%= f.label :bio %><br />
		<%= f.text_area :bio %>
	</div>
	
	<div class="actions">
		<%= f.submit "Sign up" %>
	</div>
<% end %>
